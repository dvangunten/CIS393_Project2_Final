<h1>Our Menu</h1>
<br><br><br><br><br>

<div class="menu">
    
    <table class="menutable">
        <tr>
            
            <th>Name</th>
            <th>Description</th>
            <th>Price</th>
        </tr>
        
           <% data.forEach((menuitems)=>{ %>
            <tr>
            <td><%=menuitems.Name%></td>
            <td><%=menuitems.Description%></td>
            <td><%=menuitems.Price%></td>
            </tr>
          <%  });%>
        
    </table>    

    <img class="menupic" src="https://th.bing.com/th/id/OIP.SgB8sYIxkxJj-p3Vs8MaCgHaE8?rs=1
    &pid=ImgDetMain" alt="pizza">

</div>
<div class="updatemenubutton">
    <button class="menubutton" onclick="window.location.href='/updatemenu'">Update the Menu</button>
</div>


